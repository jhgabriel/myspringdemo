<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.fly.mapper.UserMapper">



    <insert id="insertUser" parameterType="java.util.Map">
        INSERT INTO
          user
        values (null,#{username},null,null,null)
    </insert>

    <select id="selectUser" resultMap="userMap">
        SELECT
          *
        FROM
          USER
    </select>

    <resultMap id="userMap" type="com.fly.pojo.User">
        <result column="ID" javaType="long" property="id"></result>
        <result column="USERNAME" javaType="string" property="username"></result>
    </resultMap>

</mapper>